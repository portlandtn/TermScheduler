<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jedmay.termscheduler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MentorEditActivity"></activity>
        <activity android:name=".MentorListActivity" />
        <activity android:name=".NoteEditActivity" />
        <activity android:name=".NoteDetailActivity" />
        <activity android:name=".AssessmentEditActivity" />
        <activity android:name=".AssessmentDetailActivity" />
        <activity android:name=".CourseEditActivity" />
        <activity android:name=".CourseDetailActivity" />
        <activity android:name=".TermEditActivity" />
        <activity android:name=".TermDetailActivity" />
        <activity android:name=".TermListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>